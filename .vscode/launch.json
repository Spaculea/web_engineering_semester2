{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5500",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    },
    {
      "name": "Launch with Live Server",
      "request": "launch",
      "type": "chrome",
      "url": "http://127.0.0.1:5500/homePage.html",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    },
    {
      "name": "Launch Local File",
      "request": "launch",
      "type": "chrome",
      "file": "${workspaceFolder}/homePage.html",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    },
    {
      "name": "Attach to Chrome",
      "request": "attach",
      "type": "chrome",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
    }
  ]
}